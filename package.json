{
  "name": "radenpioneer.blog",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "pnpm generate-icons && astro build",
    "check": "astro sync && pnpm dlx concurrently \"pnpm exec prettier --plugin=prettier-plugin-astro --check .\" \"astro check\" \"tsc --noEmit\"",
    "dev": "astro dev",
    "format": "pnpm exec prettier --plugin=prettier-plugin-astro --write .",
    "preview": "astro preview",
    "start": "pnpm build && pnpm preview",
    "generate-icons": "pwa-assets-generator --preset minimal public/logo.jpg"
  },
  "dependencies": {
    "@astrojs/markdoc": "0.6.0",
    "@astrojs/mdx": "1.1.2",
    "@astrojs/netlify": "3.0.3",
    "@astrojs/react": "3.0.3",
    "@astrojs/rss": "3.0.0",
    "@astrojs/sitemap": "3.0.1",
    "@fontsource-variable/plus-jakarta-sans": "5.0.16",
    "@fontsource/plus-jakarta-sans": "5.0.15",
    "@radix-ui/react-avatar": "1.0.4",
    "@radix-ui/react-dropdown-menu": "2.0.6",
    "@radix-ui/react-icons": "1.3.0",
    "@resvg/resvg-js": "2.5.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "@vite-pwa/assets-generator": "0.0.10",
    "@vite-pwa/astro": "0.1.3",
    "astro": "3.3.1",
    "astro-seo": "0.8.0",
    "external-svg-loader": "1.6.10",
    "marked": "9.1.2",
    "marked-smartypants": "1.1.3",
    "postcss": "8.4.31",
    "postcss-preset-env": "9.2.0",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.69.3",
    "satori": "0.10.9",
    "satori-html": "0.3.2",
    "sharp": "0.32.6",
    "tailwindcss": "^3.3.3",
    "typescript": "5.2.2",
    "vite-plugin-pwa": "0.16.5",
    "workbox-cacheable-response": "7.0.0",
    "workbox-core": "7.0.0",
    "workbox-expiration": "7.0.0",
    "workbox-precaching": "7.0.0",

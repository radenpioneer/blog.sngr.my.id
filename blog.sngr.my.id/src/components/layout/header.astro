---
import { getImage } from 'astro:assets'
import HeaderComp from './header'
import { getEntry } from 'astro:content'
import ProfileImagePng from '~/assets/uploads/site/image.jpg'

const { data: site } = await getEntry('site', 'site')
const ProfileImage = await getImage({
  src: ProfileImagePng,
  width: 70,
  height: 70,
})
---

<HeaderComp title={site.title} image={ProfileImage} />

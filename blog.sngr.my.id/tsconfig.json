{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "types": ["vite-plugin-pwa/client"],
    "lib": ["WebWorker"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "esModuleInterop": true,
    "exactOptionalPropertyTypes": false,
    "downlevelIteration": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    }
  }
}
